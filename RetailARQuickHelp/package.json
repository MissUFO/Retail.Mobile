{
    "name": "com.adidas.retail.arquickhelp",
    "version": "1.1.1",
    "description": "Application for devices issues quick help",
    "dependencies": {
        "cordova-plugin-barcodescanner": "git+https://github.com/hypery2k/cordova-barcodescanner-plugin.git",
        "cordova-plugin-camera": "^2.4.1",
        "cordova-plugin-compat": "1.1.0",
        "cordova-plugin-console": "~1.0.3",
        "cordova-plugin-crosswalk-webview": "2.3.0",
        "cordova-plugin-device": "~1.1.2",
        "cordova-plugin-dialogs": "1.3.3",
        "cordova-plugin-file": "4.3.3",
